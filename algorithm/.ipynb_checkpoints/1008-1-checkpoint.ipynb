    "X=int(input())\n",
    "i=0\n",
    "mod=0\n",
    "while True:\n",
    "    if X<=0:\n",
    "        break\n",
    "    i+= 1\n",
    "    mod = X\n",
    "    X -= i\n",
    "#print(i, mod)\n",
    "if i % 2 == 1: # 홀수번째 카테고리면\n",
    "    print(\"{}/{}\".format(i+1-mod, mod)) #합은 i+1\n",
    "else:\n",
    "    print(\"{}/{}\".format(mod, i+1-mod))"
   ]
