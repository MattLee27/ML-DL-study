   "source": [
    "# A, B, V 입력\n",
    "# A는 낮에 올라가는 높이\n",
    "# B는 밤에 떨어지는 높이\n",
    "# V는 정상의 높이\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " 5 2 1000000\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "333333\n",
      "Wall time: 3.87 s\n"
     ]
    }
   ],
   "source": [
    "%%time\n",
    "A,B,V = map(int, input().split())\n",
    "i=0 # 날 수\n",
    "dist=0 #이동한 거리\n",
    "while True:\n",
    "    i+=1\n",
    "    dist+=A\n",
    "    if dist>=V:\n",
    "        print(i)\n",
    "        break\n",
    "    dist-=B"
   ]
  },
